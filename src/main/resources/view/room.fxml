<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="720" prefWidth="880.0" styleClass="anchor-pane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.automaatio.controller.mainpage.RoomController">
   <Button layoutX="250.0" layoutY="659.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="115.0" text="%allOnBtnTxt" />
   <Button fx:id="deleteRoom" layoutX="400.0" layoutY="659.0" mnemonicParsing="false" onAction="#deleteRoom" prefHeight="40.0" prefWidth="115.0" text="%deleteRoomBtnTxt" />
   <Button layoutX="550.0" layoutY="659.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="115.0" text="%allOffBtnTxt" />
   <ComboBox fx:id="deviceComboBox" layoutX="308.0" layoutY="543.0" maxWidth="880.0" prefHeight="25.0" prefWidth="300.0" styleClass="grid-paneDevice" />
   <Text fx:id="roomName" layoutX="372.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%roomTitleTxt">
      <font>
         <Font name="Copperplate Gothic Bold" size="24.0" />
      </font>
   </Text>
   <ScrollPane fx:id="devicesScrollPane" hbarPolicy="NEVER" AnchorPane.bottomAnchor="200.0" AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="100.0">
      <VBox fx:id="devicesVBox" maxWidth="780.0" prefWidth="780.0" />
   </ScrollPane>
</AnchorPane>
